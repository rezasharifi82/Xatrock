            <Image url="@addStudent.png" />
         </image>
      </ImageView>
      <Label layoutX="421.0" layoutY="190.0" prefHeight="71.0" prefWidth="289.0" text="Add Student">
         <font>
            <Font name="Segoe Print Bold" size="39.0" />
         </font>
      </Label>
      <ComboBox fx:id="chooseStudentComboBox" focusTraversable="false" layoutX="443.0" layoutY="521.0" prefHeight="31.0" prefWidth="228.0" promptText="student" />
      <Label layoutX="409.0" layoutY="311.0" text="Choose lesson from combo box">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
      <ComboBox fx:id="chooseLessonComboBox" focusTraversable="false" layoutX="467.0" layoutY="380.0" onAction="#chooseLessonComboBoxHandler" prefHeight="31.0" prefWidth="181.0" promptText="lessson" />
