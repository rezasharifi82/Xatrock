      <ComboBox fx:id="chooseStudentComboBox" focusTraversable="false" layoutX="530.0" layoutY="588.0" prefHeight="31.0" prefWidth="228.0" promptText="student" />
      <Label layoutX="496.0" layoutY="366.0" text="Choose lesson from combo box">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
      <ComboBox fx:id="chooseLessonComboBox" focusTraversable="false" layoutX="554.0" layoutY="435.0" onAction="#chooseLessonComboBoxHandler" prefHeight="31.0" prefWidth="181.0" promptText="lessson" />
